{
    "name": "@zyno-io/config",
    "version": "1.10.0",
    "description": "Runtime configuration encryption helpers",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "config-cli": "dist/cli.js"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./load": "./dist/load.js"
    },
    "scripts": {
        "dev": "ts-node src/cli.ts",
        "build": "rm -rf dist && tsc -p tsconfig.json && chmod +x dist/cli.js",
        "format": "npx oxlint --fix --fix-suggestions --fix-dangerously && prettier --write .",
        "test": "node --import tsx --test 'tests/**/*.spec.ts'",
        "watch": "tsc -w -p tsconfig.json"
    },
    "author": "Sean Ferguson <hello@sgnl24.com>",
    "license": "ISC",
    "dependencies": {
        "commander": "^14.0.3",
        "dotenv": "^17.3.1"
    },
    "devDependencies": {
        "@types/node": "^25.2.3",
        "lefthook": "^2.1.1",
        "oxlint": "^1.47.0",
        "prettier": "^3.8.1",
        "ts-node": "^10.9.2",
        "tslib": "^2.8.1",
        "tsx": "^4.21.0",
        "type-fest": "^5.4.4",
        "typescript": "^5.9.3"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "packageManager": "yarn@4.9.1"
}
